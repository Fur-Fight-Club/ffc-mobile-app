{
  "expo": {
    "name": "fur-fight-club",
    "version": "1.0.0",
    "sdkVersion": "48.0.0",
    "scheme": "https",
    "slug": "ffc.mistergooddeal.org",
    "hostType": "domain",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "package": "com.mistergooddeal.furfightclub",
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "ffc.mistergooddeal.org",
              "pathPrefix": "/payments"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.mistergooddeal.furfightclub",
      "associatedDomains": ["applinks:ffc.mistergooddeal.org"],
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-image-picker",
        {
          "photosPermission": "$(PRODUCT_NAME) your photos to let you share them with your monster."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c74a8dd0-9d49-4273-a9e7-b1911ce8861c"
      }
    }
  }
}
